{
    "face_detector": "opencv",
    "window_duration_secs": 5,
    "blinking": {
        "min_threshold": 0.2,
        "max_threshold": 0.6
    },
    "sounds": {
        "camera_shot": "./sounds/camera_shot.wav",
        "mic_stop": "./sounds/mic_stop.wav"
    },
    "speech": {
        "words_to_display": 3,
        "use_soundex_match": true,
        "record_duration_secs": 5
    },
    "debug": true
}