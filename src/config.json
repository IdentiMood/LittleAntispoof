{
    "face_detector": "dlib",
    "window_duration_secs": 5,
    "blinking": {
        "min_threshold": 0.6,
        "max_threshold": 1.5
    },
    "sounds": {
        "start": "./sounds/start.mp3",
        "camera_shot": "./sounds/camera_shot.wav",
        "mic_stop": "./sounds/mic_stop.mp3"
    },
    "speech": {
        "words_to_display": 3,
        "use_soundex_match": true,
        "record_duration_secs": 5
    },
    "debug": true
}